---
Language:        Cpp
BasedOnStyle: WebKit

# always attach braces to context
BreakBeforeBraces: BS_Attach

# use tabs
TabWidth:        4
UseCRLF:         false
UseTab:          Always
WhitespaceSensitiveMacros:
  - STRINGIZE
  - PP_STRINGIZE
  - BOOST_PP_STRINGIZE
  - NS_SWIFT_NAME
  - CF_SWIFT_NAME
